<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_margin="10dp"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:textColor="@color/gray"
                android:layout_marginTop="9dp"
                android:text="Product Name"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:id="@+id/divider"
                android:layout_width="390dp"
                android:layout_height="1dp"
                android:layout_marginStart="1dp"
                android:layout_marginTop="9dp"
                android:layout_marginEnd="1dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView5" />

            <TextView
                android:id="@+id/product_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="170dp"
                android:text="Leadway saving plan"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textView5" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="9dp"
                android:text="Quotation number"
                android:textColor="@color/gray"
                app:layout_constraintStart_toStartOf="@+id/textView5"
                app:layout_constraintTop_toBottomOf="@+id/divider" />

            <TextView
                android:id="@+id/quotation_number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="43979"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="@+id/product_name"
                app:layout_constraintTop_toTopOf="@+id/textView7" />

            <View
                android:id="@+id/divider2"
                android:layout_width="390dp"
                android:layout_height="1dp"
                android:layout_marginStart="1dp"
                android:layout_marginTop="9dp"
                android:layout_marginEnd="1dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="9dp"
                android:text="Frequency"
                android:textColor="@color/gray"
                app:layout_constraintStart_toStartOf="@+id/textView7"
                app:layout_constraintTop_toBottomOf="@+id/divider2" />

            <TextView
                android:id="@+id/frequency"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Monthly"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="@+id/quotation_number"
                app:layout_constraintTop_toTopOf="@+id/textView9" />

            <View
                android:id="@+id/divider3"
                android:layout_width="390dp"
                android:layout_height="1dp"
                android:layout_marginStart="1dp"
                android:layout_marginTop="9dp"
                android:layout_marginEnd="1dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView9" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="9dp"
                android:text="Quotation Date"
                android:textColor="@color/gray"
                app:layout_constraintStart_toStartOf="@+id/textView9"
                app:layout_constraintTop_toBottomOf="@+id/divider3" />

            <TextView
                android:id="@+id/quotation_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="05-apr-2019"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="@+id/frequency"
                app:layout_constraintTop_toTopOf="@+id/textView11" />

            <View
                android:id="@+id/divider4"
                android:layout_width="390dp"
                android:layout_height="1dp"
                android:layout_marginTop="9dp"
                android:layout_marginEnd="1dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView11" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="9dp"
                android:text=" Sum Assure"
                android:textColor="@color/gray"
                app:layout_constraintStart_toStartOf="@+id/textView11"
                app:layout_constraintTop_toBottomOf="@+id/divider4" />

            <TextView
                android:id="@+id/sum"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="500"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="@+id/quotation_date"
                app:layout_constraintTop_toTopOf="@+id/textView13" />

            <View
                android:id="@+id/divider5"
                android:layout_width="390dp"
                android:layout_height="1dp"
                android:layout_marginTop="9dp"
                android:layout_marginEnd="1dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView13" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="9dp"
                android:text="Premium"
                app:layout_constraintStart_toStartOf="@+id/textView13"
                app:layout_constraintTop_toBottomOf="@+id/divider5" />

            <TextView
                android:id="@+id/premium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="5,000.00"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="@+id/sum"
                app:layout_constraintTop_toTopOf="@+id/textView15" />

            <View
                android:id="@+id/divider6"
                android:layout_width="390dp"
                android:layout_height="1dp"
                android:layout_marginStart="1dp"
                android:layout_marginTop="9dp"
                android:layout_marginEnd="1dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView15" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="9dp"
                android:text="Death Cover"
                android:textColor="@color/gray"
                app:layout_constraintStart_toStartOf="@+id/textView15"
                app:layout_constraintTop_toBottomOf="@+id/divider6" />

            <TextView
                android:id="@+id/death"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nil"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="@+id/premium"
                app:layout_constraintTop_toTopOf="@+id/textView17" />

            <View
                android:id="@+id/divider7"
                android:layout_width="390dp"
                android:layout_height="1dp"
                android:layout_marginStart="1dp"
                android:layout_marginTop="9dp"
                android:layout_marginEnd="1dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView17" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="9dp"
                android:text="Critical Illness Cover"
                android:textColor="@color/gray"
                app:layout_constraintStart_toStartOf="@+id/textView17"
                app:layout_constraintTop_toBottomOf="@+id/divider7" />

            <TextView
                android:id="@+id/critical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nil"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="@+id/death"
                app:layout_constraintTop_toTopOf="@+id/textView19" />

            <View
                android:id="@+id/divider8"
                android:layout_width="390dp"
                android:layout_height="1dp"
                android:layout_marginTop="9dp"
                android:layout_marginEnd="1dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView19" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="9dp"
                android:layout_marginBottom="9dp"
                android:text="Disability"
                android:textColor="@color/gray"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/textView19"
                app:layout_constraintTop_toBottomOf="@+id/divider8" />

            <TextView
                android:id="@+id/disability"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nil"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="@+id/critical"
                app:layout_constraintTop_toTopOf="@+id/textView21" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>